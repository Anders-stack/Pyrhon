        "def isYearLeap(Year):\n",
        "  p=False\n",
        "  if Year % 4 == 0:\n",
        "    if Year % 100 == 0:\n",
        "      if Year % 400 == 0:\n",
        "        p=True\n",
        "      else:\n",
        "        p=False\n",
        "    else:\n",
        "      p=True\n",
        "  else:\n",
        "    p=False\n",
        "  return p \n",
        "\n",
        "year=int(input(\"Ingrese un año : \"))\n",
        "if isLeap(year):\n",
        "  print(\" el año:\",year,\"es bisiesto\")\n",
        "else:\n",
        "  print(\"el año:\",year,\"no es bisiesto\")"
