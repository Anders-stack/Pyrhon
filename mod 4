        "#Celda 29\n",
        "def daysInMonth(y,m):\n",
        "    days=None\n",
        "    if m==2:\n",
        "        if isLeap(y):\n",
        "            days=29\n",
        "        else:\n",
        "            days=28\n",
        "    else:\n",
        "        if (m==1)or (m==3)or (m==5)or (m==7)or (m==8)or (m==10)or (m==12):\n",
        "            days=31\n",
        "        elif (m==4)or (m==6)or (m==9)or (m==11):\n",
        "            days=30\n",
        "\n",
        "    return days           \n",
        "    \n",
        "\n",
        "y=int(input(\"ingrese el años: \"))\n",
        "m=int(input(\"ingrese el mes: \"))\n",
        "days=daysInMonth(y,m)\n",
        "print(days)"
      ],
