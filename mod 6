        "#Celda 43\n",
        "def potenciasDe2(n):\n",
        "    potencia = 1\n",
        "    for i in range(n):\n",
        "        yield potencia\n",
        "        potencia *= 2\n",
        "\n",
        "for v in potenciasDe2(8):\n",
        "    print(v)"
