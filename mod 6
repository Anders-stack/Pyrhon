        "# Celda 19\n",
        "\n",
        "\n",
        "class vehicle():\n",
        "    def __init__(self):\n",
        "        pass\n",
        "    def desplazar(self):\n",
        "        print(\"vehicle\")\n",
        "class barco(vehicle):\n",
        "    def __init__(self):\n",
        "        pass\n",
        "    def desplazar(self):\n",
        "        pass\n",
        "class coche(vehicle):\n",
        "    def __init__(self):\n",
        "        pass\n",
        "    def desplazar(self):\n",
        "        print(\"coche\")\n",
        "\n",
        "coche1=coche()\n",
        "coche1.desplazar()"
      ],
